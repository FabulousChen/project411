<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swu.chen.mapper.LoseInformationMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.swu.chen.entity.LoseInformation">
        <id column="id" property="id" />
        <result column="place" property="place" />
        <result column="item" property="item" />
        <result column="item_desc" property="itemDesc" />
        <result column="picture" property="picture" />
        <result column="people" property="people" />
        <result column="iphone" property="iphone" />
        <result column="loss_time" property="lossTime" />
        <result column="creat_time" property="creatTime" />
        <result column="mod_time" property="modTime" />
        <result column="status" property="status" />
        <result column="audit_status" property="auditStatus" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, place, item, item_desc AS itemDesc, picture, people, iphone, loss_time AS lossTime, creat_time AS creatTime, mod_time AS modTime, status, audit_status AS auditStatus, is_delete AS isDelete
    </sql>

</mapper>
