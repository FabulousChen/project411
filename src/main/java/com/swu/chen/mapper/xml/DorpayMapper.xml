<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swu.chen.mapper.DorpayMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.swu.chen.entity.Dorpay">
        <id column="id" property="id" />
        <result column="dorPayment_id" property="dorpaymentId" />
        <result column="pay_status" property="payStatus" />
        <result column="cost" property="cost" />
        <result column="dormitory_id" property="dormitoryId" />
        <result column="create_time" property="createTime" />
        <result column="pay_time" property="payTime" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dorPayment_id AS dorpaymentId, pay_status AS payStatus, cost, dormitory_id AS dormitoryId, create_time AS createTime, pay_time AS payTime, is_delete AS isDelete
    </sql>

</mapper>
